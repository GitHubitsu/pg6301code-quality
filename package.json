{
  "name": "code-quality",
  "version": "1.0.0",
  "description": "* [ ] Create react application with parcel * [ ] Jest * [ ] Snapshot tests   * Testing rendering * [ ] Simulator testint   * Testing submission * [ ] GitHub Actions * [ ] Coverage coveralls.io * [ ] Prettier * [ ] Typescript * [ ] Eslint (?)",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "test": "jest",
    "format": "prettier --write *.{jsx,js} __tests__/**/*.{jsx,js}",
    "format:check": "prettier --check *.{jsx,js} __tests__/**/*.{jsx,js}",
    "verify": "npm run format:check && jest --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitHubitsu/pg6301code-quality.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jsdom": "^22.1.0",
    "parcel": "^2.9.3",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "*.jsx"
